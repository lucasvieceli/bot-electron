{
    "name": "Bot-Bombcrypto",
    "main": "./dist-webpack/main/bundle.js",
    "version": "0.1.0",
    "email": "vieceli46@hotmail.com",
    "author": "lucas vieceli <vieceli46@hotmail.com>",
    "url": "http://barnyrubble.tumblr.com/",
    "license": "(ISC OR GPL-3.0)",
    "description": "A boilerplate for creating projects that want to use typeorm with sqlite3 inside electron.",
    "private": true,
    "engineStrict": true,
    "engines": {
        "node": "=14.18.3"
    },
    "scripts": {
        "postinstall": "electron-builder install-app-deps",
        "build": "webpack --config webpack.config.prod.js",
        "build:dev": "webpack --config webpack.config.dev.js",
        "package": "npx rimraf ./dist-webpack && npm run build && electron-builder --mac ",
        "start": "concurrently \"npm run start:dev\" \"npm run start:electron\"",
        "start1": "concurrently \"npm run start:prod\" \"npm run start:electron\"",
        "start:dev": "webpack-dev-server --config webpack.config.dev.js --port 8080 ",
        "start:prod": "webpack-dev-server --config webpack.config.prod.js --port 8080 ",
        "start:electron": "npm run build:dev && electron ./dist-webpack/main/bundle.js --trace-uncaught",
        "migration": "typeorm migration:create -d ./src/main/database/migrations -n ",
        "build:python": "npx rimraf ./bot-build/mac &&   python3.10 -m PyInstaller ./functions.py --noconsole --noconfirm --name bot  --distpath ./bot-build/mac --onefile",
        "env:python": "source ./botbomb/bin/activate"
    },
